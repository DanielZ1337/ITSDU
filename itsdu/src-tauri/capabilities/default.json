{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "platforms": [
    "linux",
    "windows",
    "macOS"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "window:allow-close",
    "window:allow-minimize",
    "window:allow-toggle-maximize",
    "window:allow-maximize",
    "process:default",
    "stronghold:default"
  ]
}